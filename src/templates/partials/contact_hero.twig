<section class="contact-hero">
    <h1 class="contact-hero__title">{{ contact.hero.title }}</h1>
    <nav class="contact-hero__nav">
        <ul class="contact-hero__nav-list">
            {% for link in contact.hero.links %}
            <li>
                <a href="{{ link.href }}" class="link link--tertiary contact-hero__nav-item {% if link.active %} contact-hero__nav-item--active {% endif %}">{{ link.label }}</a>
            </li>
              {% endfor %} 
        </ul>
    </nav>
</section>